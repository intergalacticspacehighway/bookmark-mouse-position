{
  "name": "bookmark-mouse-location",
  "version": "1.0.0",
  "main": "main.js",
  "author": "nishan",
  "license": "MIT",
  "description": "Bookmark mouse locations and go back to them when needed",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": "electron-forge start",
    "rebuild": "electron-rebuild",
    "parcel": "parcel index.html",
    "bundle": "parcel build index.html --public-url '.'",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "generate-dmg": "electron-installer-dmg ./out/bookmark-mouse-location-darwin-x64/bookmark-mouse-location.app 'Bookmark mouse position'"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "^16.0.2",
    "electron-installer-dmg": "^3.0.0",
    "electron-rebuild": "^3.2.5",
    "parcel": "^2.0.1"
  },
  "dependencies": {
    "@nut-tree/nut-js": "^1.7.1-next.20211127001917",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "bookmark_mouse_location"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
